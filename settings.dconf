[com/gexperts/Tilix]
auto-hide-mouse=true
enable-wide-handle=false
prompt-on-close=false
prompt-on-delete-profile=true
quake-specific-monitor=0
tab-position='top'
terminal-title-style='none'
unsafe-paste-alert=false
use-tabs=true
window-style='disable-csd-hide-toolbar'

[com/gexperts/Tilix/keybindings]
session-add-down='<Super>Return'
session-add-right='<Shift><Super>Return'
win-switch-to-next-session='<Super>bracketright'
win-switch-to-previous-session='<Super>bracketleft'

[com/gexperts/Tilix/profiles]
default='2b7c4080-0ddd-46c5-8f23-563fd3ba789d'
list=['2b7c4080-0ddd-46c5-8f23-563fd3ba789d']

[com/gexperts/Tilix/profiles/2b7c4080-0ddd-46c5-8f23-563fd3ba789d]
background-color='#282C34'
badge-color='#AC7EA8'
badge-color-set=false
badge-position='northeast'
badge-text=''
bold-color-set=false
bold-is-bright=true
cursor-background-color='#A1B0B8'
cursor-colors-set=false
cursor-foreground-color='#BDC3C7'
dim-transparency-percent=12
font='Hack 16'
foreground-color='#ABB2BF'
highlight-colors-set=false
palette=['#000000', '#E06C75', '#98C379', '#D19A66', '#61AEEE', '#C678DD', '#56B6C2', '#ABB2BF', '#5C6370', '#E06C75', '#98C379', '#D19A66', '#62AFEE', '#C678DD', '#56B6C2', '#FFFFFF']
scrollback-unlimited=true
use-system-font=false
use-theme-colors=false
visible-name='Default'

[org/gnome/desktop/input-sources]
xkb-options=['caps:ctrl_modifier']

[org/gnome/desktop/peripherals/touchpad]
two-finger-scrolling-enabled=true

[org/gnome/settings-daemon/plugins/media-keys]
custom-keybindings=['/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom0/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom1/']

[org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom0]
binding='<Shift><Super>s'
command='flameshot gui'
name='Screenshot'

[org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom1]
binding='<Super>Eject'
command='sh -c "~/.dotfiles/scripts/toggle-vpn"'
name='Toggle VPN'

[org/gnome/shell]
app-picker-view=uint32 0
disabled-extensions=['ubuntu-appindicators@ubuntu.com']
enabled-extensions=@as []
favorite-apps=['com.gexperts.Tilix.desktop', 'google-chrome.desktop', 'discord.desktop', 'slack.desktop', 'spotify.desktop', 'enpass.desktop']

[org/gnome/shell/extensions/dash-to-dock]
dock-position='BOTTOM'
show-mounts=false

[org/gnome/shell/extensions/desktop-icons]
show-home=false
show-trash=false
